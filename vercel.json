{
  "builds": [
    {
      "src": "frontend_v2/**/*",
      "use": "@vercel/static-build",
      "config": { "distDir": "frontend_v2/build" }
    },
    {
      "src": "frontend_v3/**/*",
      "use": "@vercel/static-build",
      "config": { "distDir": "frontend_v3/build" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "$VERCEL_BRANCH == 'v3_design' ? '/frontend_v3/$1' : '/frontend_v2/$1'"
    }
  ]
}
