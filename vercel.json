{
  "builds": [
    {
      "src": "frontend_v2/package.json",
      "use": "@vercel/next",
      "config": { "distDir": "frontend_v2/.next" }
    },
    {
      "src": "frontend_v3/package.json",
      "use": "@vercel/next",
      "config": { "distDir": "frontend_v3/.next" }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/frontend_v2/$1"
    }
  ]
}
